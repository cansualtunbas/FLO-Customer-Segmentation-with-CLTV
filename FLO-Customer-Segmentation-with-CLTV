lace_with_thresholds(dataframe, variable):\n    low_limit, up_limit = outlier_thresholds(dataframe, variable)\n    dataframe.loc[(dataframe[variable] < low_limit), variable] = low_limit\n    dataframe.loc[(dataframe[variable] > up_limit), variable] = up_limit\n    ","metadata":{"execution":{"iopub.status.busy":"2024-09-09T16:24:58.794296Z","iopub.execute_input":"2024-09-09T16:24:58.795293Z","iopub.status.idle":"2024-09-09T16:24:58.803747Z","shell.execute_reply.started":"2024-09-09T16:24:58.795244Z","shell.execute_reply":"2024-09-09T16:24:58.802348Z"},"trusted":true},"execution_count":4,"outputs":[]},{"cell_type":"code","source":"#Adım3:  \"order_num_total_ever_online\", \"order_num_total_ever_offline\", \"customer_value_total_ever_offline\",  \"customer_value_total_ever_online\" değişkenlerinin aykırı değerleri varsa baskılayanız.\ndata=[\"order_num_total_ever_online\",\"order_num_total_ever_offline\",\"customer_value_total_ever_offline\",\"customer_value_total_ever_online\"]\ndf[data].describe().T\n","metadata":{"execution":{"iopub.status.busy":"2024-09-09T16:24:58.806823Z","iopub.execute_input":"2024-09-09T16:24:58.807262Z","iopub.status.idle":"2024-09-09T16:24:58.862936Z","shell.execute_reply.started":"2024-09-09T16:24:58.807218Z","shell.execute_reply":"2024-09-09T16:24:58.861852Z"},"trusted":true},"execution_count":5,"outputs":[{"execution_count":5,"output_type":"execute_result","data":{"text/plain":"                                       count     mean      std     min      25%      50%      75%        max\norder_num_total_ever_online       19945.0000   3.1109   4.2256  1.0000   1.0000   2.0000   4.0000   200.0000\norder_num_total_ever_offline      19945.0000   1.9139   2.0629  1.0000   1.0000   1.0000   2.0000   109.0000\ncustomer_value_total_ever_offline 19945.0000 253.9226 301.5329 10.0000  99.9900 179.9800 319.9700 18119.1400\ncustomer_value_total_ever_online  19945.0000 497.3217 832.6019 12.9900 149.9800 286.4600 578.4400 45220.1300","text/html":"<div>\n<s